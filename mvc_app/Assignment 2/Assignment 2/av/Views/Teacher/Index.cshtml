@model IEnumerable<av.Models.Teacher>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-8">
            <h2 class="text-primary">Teacher List</h2>
        </div>
    </div>
    <br/>
    @if(Model.Count() > 0){
                <table class="table table-bordered table-striped" style="width:100%">
                    <thead>
                        <tr>
                            <th>Teacher Id</th><th>Teacher Name</th><th>Subject</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach(var obj1 in Model){
                        <tr>
                            <td width="50%">@obj1.TeacherId</td>
                            <td width="50%">@obj1.Teacher_Name</td>
                            <td width="50%">@obj1.Subject</td>
                        </tr>
                    }
                    </tbody>
                    @* <tbody>
                        <input type="button" value="">
                    </tbody> *@
                </table>
    }
    else{
    <p> No Teacher details.</p>
    }
    </div>
